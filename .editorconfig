# top-most EditorConfig file
root = true

[*]
# [encoding-utf8]
charset = utf-8

# [newline-lf]
end_of_line = lf

# [newline-eof]
insert_final_newline = true

# [no-trailing-spaces]
trim_trailing_whitespace = true

[*.md]
indent_style = space
indent_size = 4
tab_width = 4

[*.bat]
end_of_line = crlf

[*.java]
# [indentation-tab]
indent_style = tab

# [4-spaces-tab]
indent_size = 4
tab_width = 4

[line-length-120]
max_line_length = 120
